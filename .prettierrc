{
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "overrides": [{
    "files": "*.json",
    "options": {
      "parser": "json",
      "tabWidth": 2
    }
  }],
  "parser": "babylon",
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "all",
  "useTabs": false
}
